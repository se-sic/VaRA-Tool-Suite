{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug TestDriverPlot",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/varats/varats/tools/driver_plot.py",
            "args": [
                "fperf-stat"
            ],
            "justMyCode": false,
            "console": "integratedTerminal",
        },
        {
            "name": "Debug driver-run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/varats/varats/tools/driver_run.py",
            "args": [
                "-E PerfStat"
            ],
            "cwd": "~/vara",
            "justMyCode": false,
            "console": "integratedTerminal",
        },
        {
            "name": "benchbuild",
            "type": "python",
            "request": "launch",
            "program": "~/.local/bin/benchbuild",
            "cwd": "~/vara/benchbuild",
            "args": [
                "run",
                "-E",
                "PerfStat",
            ],
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ]
}
// This configuration needs vara-root next to VaRA-Tool-Suite.
// You can pick any experiment instead of JustCompile and any project for gzip.
// Set a breakpoint with F9 (for this example) in experiments/base/just_compile.py.
// Press F5 to run benchbuild with the defined args[].
